using System;

namespace shuffle
{
    class Program
    {
        static void Main(string[] args)
        {
            string[] array = { "Антон", "Борис", "Вера", "Григорий", "Дмитрий", "Елена", "Жозефина", "Зинаида", "Ирина", "Катерина" };

            for (int i = 0; i < array.Length; i++)
            {
                Console.Write(array[i] + " ");
            }
            Console.WriteLine();
            string[] mixArray = Shuffle(array);
            for (int i = 0; i < mixArray.Length; i++)
            {
                Console.Write(mixArray[i] + " ");
            }
            Console.WriteLine();
        }
        static string[] Shuffle(string[] array)
        {
            Random random = new Random();
            int randNumber;
            string copyElementArray;

            for (int i = 0; i < array.Length; i++)
            {
                randNumber = random.Next(0, 10);
                copyElementArray = array[i];
                array[i] = array[randNumber];
                array[randNumber] = copyElementArray;
            }
            return array;
        }
    }
}
